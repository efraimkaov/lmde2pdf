<office:master-styles>
<style:master-page style:name="fp" style:page-layout-name="Mpm1" style:next-style-name="#secondPage#"/>
<style:master-page style:name="lp" style:page-layout-name="Mpm2" style:next-style-name="rp">
  <style:header>
    <text:p text:style-name="hd"><text:page-number text:select-page="current"></text:page-number><text:tab/>#leftHeader#</text:p>
  </style:header>
</style:master-page>
<style:master-page style:name="rp" style:page-layout-name="Mpm2" style:next-style-name="lp">
  <style:header>
    <text:p text:style-name="hd">#rightHeader#<text:tab/><text:page-number text:select-page="current"></text:page-number></text:p>
  </style:header>
</style:master-page>
</office:master-styles>
